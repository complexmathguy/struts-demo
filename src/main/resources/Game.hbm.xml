<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping> 
<!-- AIB : #hibernateClassDeclaration() -->        
   <class name="com.harbormaster.bo.Game" 
         table="game"
         abstract="false">         
     <id name="gameId" type="long" >
      <generator class="native"/>    
    </id>
       <property name="frames" column="frames" not-null="false"/>
     
       
           <many-to-one name="matchup" cascade="save-update"/>
         
 	         <many-to-one name="player" cascade="save-update"/>
             </class>
<!-- ~AIB -->

</hibernate-mapping>
